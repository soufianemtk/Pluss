/* Encadré principal */
.mdl-card {
	position:absolute;
	margin-left: calc(50% - 165px);
	margin-top:10em;
	z-index: 10;
	border-radius: 2px;
}


/* button login*/
div.card_login {
	padding: 6px 20px 6px 20px;
}


/* button signin*/
div.card_signup {
	padding: 6px 20px 6px 20px;
}

/*Lost pwd link */
a.lost_pwd.link {
	float: right;
	text-decoration: none;
  	color: #C6C6C6;
  	font-size: 15px;
  	font-family: 'Arial', arial;
  	margin-top: 5px;
  	margin-bottom: -25px;
}
a.lost_pwd.link:hover {
	text-decoration: underline;
  	color: #039BE5;
}

/**** Effet JS encadrés ****/

div.encadre_utilisateur {
	padding: 30px 20px 30px 20px;
}

/* erreur entrée mot de passe*/
.log-status.wrong-entry {
  -moz-animation: wrong-log 0.3s;
  -webkit-animation: wrong-log 0.3s;
  animation: wrong-log 0.3s;
}
.log-status.wrong-entry .form-control, .wrong-entry .form-control + .fa {
  border-color: #ed1c24;
  color: #ed1c24;
}
/* message d'alerte de l'identifiant erroné*/
.alert {
  display: none;
  margin-top: 5px;
  font-size: 15px;
  font-family: 'Arial', arial;
  color: #f00;
  float: left;
}

/* surbrillance lors de l'écriture dans encadrés*/
.form-control:focus, .form-control:focus + .fa {
  border-color: #039BE5;
  color: #039BE5;
}
.mdl-textfield__label::after {
	background-color: #039BE5;
}

/* Animation changement d'image*/

@-webkit-keyframes cf4FadeInOut {
 0% {
   opacity:1;
 }
 17% {
   opacity:1;
 }
 25% {
   opacity:0;
 }
 92% {
   opacity:0;
 }
 100% {
   opacity:1;
 }
}

@-moz-keyframes cf4FadeInOut {
 0% {
   opacity:1;
 }
 17% {
   opacity:1;
 }
 25% {
   opacity:0;
 }
 92% {
   opacity:0;
 }
 100% {
   opacity:1;
 }
}

@-o-keyframes cf4FadeInOut {
 0% {
   opacity:1;
 }
 17% {
   opacity:1;
 }
 25% {
   opacity:0;
 }
 92% {
   opacity:0;
 }
 100% {
   opacity:1;
 }
}

@keyframes cf4FadeInOut {
 0% {
   opacity:1;
 }
 17% {
   opacity:1;
 }
 25% {
   opacity:0;
 }
 92% {
   opacity:0;
 }
 100% {
   opacity:1;
 }
}


#cf4a img {
  position:absolute;
  left:0;
}

#cf4a img {
  -webkit-animation-name: cf4FadeInOut;
  -webkit-animation-timing-function: ease-in-out;
  -webkit-animation-iteration-count: infinite;
  -webkit-animation-duration: 8s;

  -moz-animation-name: cf4FadeInOut;
  -moz-animation-timing-function: ease-in-out;
  -moz-animation-iteration-count: infinite;
  -moz-animation-duration: 8s;

  -o-animation-name: cf4FadeInOut;
  -o-animation-timing-function: ease-in-out;
  -o-animation-iteration-count: infinite;
  -o-animation-duration: 8s;

  animation-name: cf4FadeInOut;
  animation-timing-function: ease-in-out;
  animation-iteration-count: infinite;
  animation-duration: 30s;
}
#cf4a img:nth-of-type(1) {
  -webkit-animation-delay: 25s;
  -moz-animation-delay: 25s;
  -o-animation-delay: 25s;
  animation-delay: 25s;
}
#cf4a img:nth-of-type(2) {
  -webkit-animation-delay: 20s;
  -moz-animation-delay: 20s;
  -o-animation-delay: 20s;
  animation-delay: 20s;
}
#cf4a img:nth-of-type(3) {
  -webkit-animation-delay: 15s;
  -moz-animation-delay: 15s;
  -o-animation-delay: 15s;
  animation-delay: 15s;
}
#cf4a img:nth-of-type(4) {
  -webkit-animation-delay: 10s;
  -moz-animation-delay: 10s;
  -o-animation-delay: 10s;
  animation-delay: 10s;
}
#cf4a img:nth-of-type(5) {
  -webkit-animation-delay: 5s;
  -moz-animation-delay: 5s;
  -o-animation-delay: 5s;
  animation-delay: 5s;
}
#cf4a img:nth-of-type(6) {
  -webkit-animation-delay: 0;
  -moz-animation-delay: 0;
  -o-animation-delay: 0;
  animation-delay: 0;
}



